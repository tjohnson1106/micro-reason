bsdep = /home/thomas/Code/web/reason/reason-blog/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -nostdlib -I '/home/thomas/Code/web/reason/reason-blog/node_modules/bs-platform/lib/ocaml' -bs-super-errors -color always
